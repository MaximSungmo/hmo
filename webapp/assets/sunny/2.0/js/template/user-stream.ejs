	<div>
		<div class="d-fuc-l-i">
		<%
			var count = data.contents.length;
			for( var i = 0; i < count; i++ ) {
				var
				user = data.contents[ i ];
		%>
				<div class="item">
					<div class="item-inner">
						<div class="item-c-col-l">
							<a class="a-prof-pic" href="/user/<%= user.id %>">
								<img class="img i-prof-pic" alt="" aria-label="<%= user.name %>" src="<%= user.profilePic %>">
							</a>
							<div class="item-c-col">
								<a href="/user/<%= user.id %>"><h3 class="n-prof"><%= user.name %></h3></a>
								<div>
									<span></span>
								</div>
							</div>
						</div>
						<div class="item-c-col-r" style="top:18px">
							<input class="h-checkbox" type="checkbox" id="user-chkbox-<%= user.id %>" name="invite-checked" value="<%= user.id %>"/>
							<label class="checkbox-label" for="user-chkbox-<%= user.id %>" aria-control="user-chkbox-<%= user.id %>">&nbsp;</label>
						</div>									
					</div>
				</div>
		<%
			}
		%>
		</div>
	</div>